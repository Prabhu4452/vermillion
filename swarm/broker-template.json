{
    "deploy": {
        "labels": {
            "type": "broker"
        },
        "placement": {
            "constraints": [
                "node.role == worker"
            ]
        }
    },
    "env_file": [
        ".env"
    ],
    "hostname": "broker",
    "image": "iudx/rabbitmq",
    "networks": [
        "iudx-net"
    ],
    "ports": [
        "15672:15672"
    ],
    "volumes": [
        "../authenticator:/authenticator/",
        "../conf/rabbitmq:/etc/rabbitmq/"
    ]
}
