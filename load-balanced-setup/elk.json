{
        "elk": {
            "env_file": [
                ".env"
            ],
            "hostname": "elk",
	    "deploy": {
                "labels": {
                    "type": "database"
                },
                "placement": {
                    "constraints": [
                        "node.role == worker",
			"node.labels.type != broker"
                    ]
                }
            },
            "image": "nshou/elasticsearch-kibana",
            "networks": [
                "iudx-net"
            ],
            "ports": [
                "9200:9200",
                "5601:5601"
            ],
            "volumes": [
                "elk-data:/home/ideam/elasticsearch-6.2.4"
            ]
        }
}
