{
    "deploy": {
        "labels": {
            "type": "broker"
        },
        "placement": {
            "constraints": [
                "node.role == worker"
            ]
        }
    },
    "env_file": [
        ".env"
    ],
    "hostname": "rabbit1",
    "image": "rabbitmq:3.7.18",
    "networks": [
        "iudx-net"
    ],
    "ports": [
        "15672:15672",
        "5672:5672"
    ],
    "volumes": [
        "../conf/rabbitmq:/etc/rabbitmq/"
    ]
}
