{
        "dbconnector": {
            "deploy": {
                "labels": {
                    "type": "dbconnector"
                },
                "placement": {
                    "constraints": [
                        "node.role == worker",
                        "node.labels.type == database"
                    ]
                }
            },
            "env_file": [
                ".env"
            ],
            "hostname": "dbconnector",
            "image": "iudx/dbconnector",
            "networks": [
                "iudx-net"
            ],
            "volumes": [
                "../iudx-dbconnector:/iudx-dbconnector/"
            ]
        }
}
